<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:gxt="urn:import:com.extjs.gxt.ui.client.widget">
			<ui:with type="com.covoiturage.client.i18n.MenuViewConstants" field="constants"/>
	<ui:style src="stylesheet.css"/>
		
	<gxt:ContentPanel ui:field='menuPanel' heading="{constants.menutitle}">
		<g:Label ui:field='mapLabel' styleName='{style.buttonmenu}' text="{constants.maplabel}"/>
  		<g:Label ui:field='settingsLabel' styleName='{style.buttonmenu}' text="{constants.settings}"/>
  		<g:Label ui:field='messagesLabel' styleName='{style.buttonmenu}' text="{constants.message}"/>
  		<g:Label ui:field='historyLabel' styleName='{style.buttonmenu}' text="{constants.history}"/>
	</gxt:ContentPanel>
</ui:UiBinder>