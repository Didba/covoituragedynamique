<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:gxt="urn:import:com.extjs.gxt.ui.client.widget">
			<ui:with type="com.covoiturage.client.i18n.HeaderViewConstants" field="constants"/>

		<ui:with type="com.covoiturage.client.resource.CovoiturageResources" field="resources"/>


  	<g:HorizontalPanel ui:field='menu'  width="100%" height="100%">
  	

  		<gxt:Label ui:field='currentUser' text="{constants.username}"/>
  		<gxt:Label ui:field='messages' title="{constants.message}"/>
  		<g:Anchor ui:field='logout' styleName='{resources.style.anchor}'  text="{constants.logout}"/>
  	</g:HorizontalPanel>

  	</ui:UiBinder>