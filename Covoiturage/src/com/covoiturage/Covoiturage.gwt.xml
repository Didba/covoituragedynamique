<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='covoiturage'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>
  <inherits name='com.google.gwt.maps.Maps' />
  <script src="http://maps.google.com/maps/api/js?sensor=false" />

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
 <inherits name='com.google.gwt.user.theme.standard.Standard'/>
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->
  
  
<inherits name='com.google.gwt.activity.Activity' /> 
<inherits name="com.google.gwt.place.Place"/>
<inherits name='com.google.web.bindery.requestfactory.RequestFactory'/>
<inherits name="com.covoiturage.GXT"/>
<inherits name="com.extjs.gxt.ui.WidgetExpander" />
<inherits name="com.jhickman.web.gwt.gxtuibinder.GxtUiBinder" />
<inherits name="com.google.gwt.appengine.channel.Channel" />
<inherits name="com.googlecode.objectify.Objectify" />

  <entry-point class='com.covoiturage.client.Covoiturage'/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>
  <source path='server.domain'/>
  
   
  <inherits name="com.google.gwt.i18n.I18N"/>
<extend-property name="locale" values="nl"/>
<extend-property name="locale" values="fr"/>
<extend-property name="locale" values="it"/>
<extend-property name="locale" values="ch"/>


</module>